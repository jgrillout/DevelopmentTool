CREATE TABLE `dataDictionary` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `tableId` varchar(15) DEFAULT NULL,
  `fieldId` varchar(20) DEFAULT NULL,
  `idFieldsid` int(11) DEFAULT NULL,
  `idTablesid` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=18 DEFAULT CHARSET=latin1;

CREATE TABLE `fields` (
  `idFieldsid` int(11) NOT NULL AUTO_INCREMENT,
  `fieldId` varchar(20) DEFAULT NULL,
  `Name` varchar(45) DEFAULT NULL,
  `Type` int(1) DEFAULT NULL,
  `Length` int(1) DEFAULT NULL,
  `FieldPrecision` int(1) DEFAULT NULL,
  `isPrimaryKey` varchar(3) DEFAULT NULL,
  `isRequired` varchar(3) DEFAULT NULL,
  `EmumValues` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`idFieldsid`)
) ENGINE=InnoDB AUTO_INCREMENT=36 DEFAULT CHARSET=latin1;

CREATE TABLE `screenFields` (
  `idscreenFields` int(11) NOT NULL AUTO_INCREMENT,
  `ScreenLayoutId` varchar(10) DEFAULT NULL,
  `ScreenId` varchar(10) DEFAULT NULL,
  `tableId` varchar(15) DEFAULT NULL,
  `FieldId` varchar(20) DEFAULT NULL,
  `fieldName` varchar(45) DEFAULT NULL,
  `labelText` varchar(45) DEFAULT NULL,
  `labelRow` int(11) DEFAULT NULL,
  `labelCol` int(11) DEFAULT NULL,
  `fieldType` int(11) DEFAULT NULL,
  `fieldRow` int(11) DEFAULT NULL,
  `fieldCol` int(11) DEFAULT NULL,
  `fieldLen` int(11) DEFAULT NULL,
  `fieldPrec` int(11) DEFAULT NULL,
  `fieldEditable` varchar(1) DEFAULT NULL,
  `fieldRequired` varchar(1) DEFAULT NULL,
  `fieldTabOrder` varchar(1) DEFAULT NULL,
  PRIMARY KEY (`idscreenFields`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

CREATE TABLE `screens` (
  `idscreens` int(11) NOT NULL AUTO_INCREMENT,
  `ScreenLayoutId` varchar(20) DEFAULT NULL,
  `ScreenName` varchar(30) DEFAULT NULL,
  `tableId` varchar(15) DEFAULT NULL,
  PRIMARY KEY (`idscreens`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

CREATE TABLE `tables` (
  `idTablesid` int(11) NOT NULL AUTO_INCREMENT,
  `tableId` varchar(15) DEFAULT NULL,
  `tableName` varchar(45) DEFAULT NULL,
  `database` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`idTablesid`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=latin1;





